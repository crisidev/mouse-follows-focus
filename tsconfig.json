{
  "extends": ["@tsconfig/strictest/tsconfig.json"],
  "compilerOptions": {
    "target": "ES2017",
    "module": "ES6",
    "lib": ["ES2017"],
    "removeComments": false,
    "paths": {
      "resource:///org/gnome/shell/*": ["./@types/gnome-shell/*"]
    }
  },
  "include": ["*.ts", "@types/**/*.ts"],
  "exclude": [".ts-for-girrc.js", ".eslintrc.cjs"]
}
